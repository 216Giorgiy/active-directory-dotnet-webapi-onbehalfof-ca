<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <add key="ida:Tenant" value="[YourTenant e.g. tenant.onmicrosoft.com]" />
    <add key="ida:ClientId" value="[Native App App/Client ID]" />
    <add key="ida:RedirectUri" value="http://TodoListClient" />
    <add key="todo:TodoListResourceId" value="[App ID URI of Web Service e.g. https://tenant/TodoListService]" />
    <add key="ida:AADInstance" value="https://login.microsoftonline.com/{0}" />
    <add key="todo:TodoListBaseAddress" value="https://localhost:44321" />
  </appSettings>
</configuration>